from bs4 import BeautifulSoup
import requests
html_text = requests.get("https://www.pakwheels.com/used-cars/automatic/57336").text
soup = BeautifulSoup(html_text,'lxml')


# cars = soup.find_all('li', class_ ='classified-listing featured-listing')
#
# car_Name = soup.find_all('a', class_='search-title-row')

classified_listing = soup.find("li", class_="classified-listing featured-listing")
search_list = classified_listing.find("div", class_="well search-list clearfix ad-container page-")
grid_style = search_list.find("div", class_="col-md-9 grid-style")
csl= grid_style.find("div",class_="")
search_title_row = csl.find("div", class_="search-title-row")
search_title = search_title_row.find("div", class_="search-title-row")
car_name = search_title_row.find("a", class_="car-name ad-detail-path")
title = car_name.find("title").text

print(title)
# print(car_Name)